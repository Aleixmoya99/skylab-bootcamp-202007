<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<title>Dasboard</title>
		<link rel="stylesheet" href="css/styles.css" />
		<link rel="stylesheet" href="css/dashboard.css" />
	</head>
	<body>
		<%-include('header')-%>
		<h1>Top Skylabers</h1>
		<div class="grid grid-pad">
			<% skylabers.forEach(element => { %>
			<a class="col-1-4" href="/skylabers/<%=element.id%>">
				<div class="module skylaber">
					<h4><%= element.name %></h4>
				</div>
			</a>
			<%}) %>
		</div>
	</body>
</html>
