    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>dashboard</title>
    <link rel="stylesheet" href="../css/index.css">
</head>

<body>
    <%- include('laberNav'); -%>
    <h3><%= title %></h3>
